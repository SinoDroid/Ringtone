server {
	listen   8002; ## listen for ipv4; this line is default and implied
	listen   [::]:80 default_server ipv6only=on; ## listen for ipv6

	server_name localhost;

	location / {
		include uwsgi_params;
		uwsgi_pass 127.0.0.1:8001;	
	}

    location /media/ {
        alias /var/www/Ringtone/res/media/;
        autoindex on;
        allow 127.0.0.1;
        allow ::1;
        deny all;
    }
}
